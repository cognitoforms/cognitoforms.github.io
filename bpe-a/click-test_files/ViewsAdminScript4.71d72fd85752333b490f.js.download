			}

			Cognito.Forms.updateEntryReadState({ Entries: entries, Token: Cognito.Forms.model.currentView.get_Token() }, read,
				function (data, read) {
					controller.detail.changeReadStates(entries, read);
					controller.master.updateProgressMessage();
				},
				function () {
					controller.master.updateProgressMessage();
				}
			);
		}

